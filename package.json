		"@quasar/vite-plugin": "^1.0.5",
		"@types/lodash": "^4.14.180",
		"@typescript-eslint/eslint-plugin": "^5.16.0",
		"@typescript-eslint/parser": "^5.16.0",
		"@vitejs/plugin-vue": "^2.2.0",
		"eslint": "^8.12.0",
		"eslint-config-prettier": "^8.5.0",
		"prettier": "^2.6.1",
		"quasar": "^2.5.5",
		"sass": "1.32.0",
		"typescript": "^4.5.4",
