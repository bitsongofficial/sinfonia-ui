{
	"name": "sinfonia-ui",
	"private": true,
	"version": "0.0.0",
	"scripts": {
		"dev": "vite",
		"dev:mainet": "vite --mode mainnet",
		"build": "vue-tsc --noEmit && vite build",
		"build:mainnet": "vue-tsc --noEmit && vite build --mode mainnet",
		"preview": "vite preview",
		"type-check": "vue-tsc --noEmit",
		"lint": "eslint --ignore-path .gitignore --ignore-path .eslintignore --fix src --ext .js --ext .ts ",
		"format": "prettier --ignore-path .prettierignore --write ."
	},
	"engines": {
		"node": ">=16.14.0"
	},
	"precommit": [
		"type-check",
		"lint"
	],
	"dependencies": {
		"@bitsongjs/sdk": "^1.0.0-dev-14",
		"@chenfengyuan/vue-countdown": "2",
		"@cosmjs/encoding": "^0.28.0",
		"@cosmjs/proto-signing": "^0.28.0",
		"@cosmjs/stargate": "^0.28.0",
		"@quasar/extras": "^1.12.5",
		"axios": "^0.26.1",
		"bech32": "^2.0.0",
		"bignumber.js": "^9.0.2",
		"cosmjs-types": "^0.4.1",
		"date-fns": "^2.28.0",
		"decimal.js": "^10.3.1",
		"duration-fns": "^3.0.0",
		"lodash": "^4.17.21",
		"long": "^5.2.0",
		"path": "^0.12.7",
		"pinia": "^2.0.11",
		"pinia-plugin-persistedstate-2": "^1.0.0",
